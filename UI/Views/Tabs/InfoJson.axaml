<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:Material.Styles.Controls;assembly=Material.Styles"
             xmlns:extensions="clr-namespace:MuseDashModToolsUI.Extensions"
             xmlns:viewModels="clr-namespace:MuseDashModToolsUI.Contracts.ViewModels"
             xmlns:tabs="clr-namespace:MuseDashModToolsUI.ViewModels.Tabs"
             xmlns:cc="clr-namespace:MuseDashModToolsUI.Views.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MuseDashModToolsUI.Views.Tabs.InfoJson"
             x:DataType="tabs:InfoJsonViewModel"
             DataContext="{extensions:DependencyInjection Type=viewModels:IInfoJsonViewModel}">

    <controls:Card Margin="10,0,10,10">
        <cc:TitledTextBox TitleText="{extensions:LocalizeExtensions XAML_InfoJson_ChartName}" />
    </controls:Card>
</UserControl>